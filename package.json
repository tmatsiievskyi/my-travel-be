{
  "name": "my-travel-be",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start:dev": "tsx watch ./src/index.ts",
    "knex": "tsx ./node_modules/knex/bin/cli.js --knexfile ./src/data/knexfile.ts",
    "db:migration:create": "npm run knex migrate:make",
    "db:migration:rollback": "npm run knex migrate:rollback",
    "db:migration:run": "npm run knex migrate:latest",
    "db:seed:make": "npm run knex seed:make",
    "db:seed:run": "npm run knex seed:run",
    "docker:restart": "docker-compose up --build --force-recreate --no-deps",
    "docker:run:be": "docker-compose run service-be",
    "docker:db:migration:run": "npm run docker:run:be && npm run db:migration:run",
    "lint": "eslint ./src -c .eslintrc.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "pg": "^8.11.5",
    "pino": "^9.1.0",
    "pino-pretty": "^11.0.0",
    "redis": "^4.6.13",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.12",
    "@types/pg": "^8.11.6",
    "eslint": "^9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.10.2",
    "typescript": "^5.4.5"
  }
}
